# AI Orchestrator 환경변수

# OpenAI API Key (필수)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Core API URL
CORE_API_URL=http://localhost:8080

# PostgreSQL (RAG용)
DATABASE_URL=postgresql://chatops_user:chatops_pass@localhost:5432/chatops

# LLM 설정
LLM_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0

# RAG 설정
RAG_ENABLED=true
RAG_TOP_K=3
RAG_MIN_SIMILARITY=0.5

# 로깅 레벨
LOG_LEVEL=INFO
